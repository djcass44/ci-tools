{
  "Args": [
    "--bare",
    "--sbom=none",
    "-t",
    ".",
    "build",
    "latest,main,deadbeef,v1.2.3"
  ],
  "Command": "ko",
  "Dir": "/builds/foo/bar",
  "Env": [
    "GOFLAGS=-buildvcs=false -buildmode=pie -trimpath",
    "KOCACHE=/builds/foo/bar/.cache",
    "KO_DEFAULTBASEIMAGE=registry.gitlab.example.com/foo/base-images/run:latest",
    "KO_DOCKER_REPO=foo/bar"
  ]
}